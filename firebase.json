{
  "functions": {
    "source": "src/functions"
  },
  "hosting": {
    "public": "src/public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/",
        "function": "index"
      },
      {
        "source": "/index",
        "function": "index"
      },
      {
        "source": "/privacy-policy",
        "function": "privacyPolicy"
      },
      {
        "source": "/terms-of-service",
        "function": "termsOfService"
      },
      {
        "source": "/_next/static/**",
        "function": "static"
      },
      {
        "source": "/handleNewLead",
        "function": "handleNewLead"
      }
    ]
  }
}

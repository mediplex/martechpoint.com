{
  "functions": {
    "source": "src/functions"
  },
  "hosting": {
    "public": "src/public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/handleNewLead",
        "function": "handleNewLead"
      },
      {
        "source": "/error",
        "function": "error"
      }
    ],
  "headers": [ {
    "source": "**/*.@(jpg|jpeg|gif|png|css|js|ico|woff|woff2)",
    "headers": [ {
      "key": "Cache-Control",
      "value": "public, max-age=31536000, s-maxage=31536000"
    } ]
  }]
    
  }
}
